<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ page import="user.UserDAO" %>
<%
    String userID = request.getParameter("userID");
    UserDAO userDAO = new UserDAO();
    boolean isExist = userDAO.isUserIdExist(userID);

    if (isExist) {
        out.print("EXIST");
    } else {
        out.print("OK");
    }
%>
